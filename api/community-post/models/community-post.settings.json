{
  "kind": "collectionType",
  "collectionName": "community_posts",
  "info": {
    "name": "community-post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "location": {
      "type": "string"
    },
    "votes": {
      "type": "integer",
      "default": 0
    },
    "phone": {
      "type": "biginteger",
      "required": false
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    }
  }
}
